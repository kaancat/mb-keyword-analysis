# mb-keyword-analysis Plugin Credentials
# ========================================
# Copy this file to: ~/.mondaybrew/.env
# All services load credentials from this central location.

# Google Ads API Credentials
GOOGLE_ADS_DEVELOPER_TOKEN=
GOOGLE_ADS_CLIENT_ID=
GOOGLE_ADS_CLIENT_SECRET=
GOOGLE_ADS_REFRESH_TOKEN=
GOOGLE_ADS_LOGIN_CUSTOMER_ID=
GOOGLE_ADS_CUSTOMER_ID=

# Google Cloud Credentials (for BigQuery)
# Place service-account-key.json in ~/.mondaybrew/
GOOGLE_APPLICATION_CREDENTIALS=/Users/YOUR_USERNAME/.mondaybrew/service-account-key.json
BIGQUERY_PROJECT_ID=

# AI Configuration
OPENAI_API_KEY=
GEMINI_API_KEY=

# Optional: Facebook Conversions API (for offline conversion upload)
FACEBOOK_ACCESS_TOKEN=
